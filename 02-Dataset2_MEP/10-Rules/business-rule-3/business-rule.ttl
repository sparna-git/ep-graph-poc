epsh:PoliticalGroup a sh:NodeShape ;
	sh:targetNode eponto:MEP ;
	sh:sparql [
		a sh:SPARQLConstraint ;
		skos:definition "The number of active members is at least 705." ;
		sh:message "Not enough MEP ({$nbMep}), was expecting at least 705" ;
		sh:select """
		prefix ep: <http://data.europarl.europa.eu/> 
		prefix eponto: <http://data.europarl.europa.eu/ontology/ep#> 
		prefix org: <http://www.w3.org/ns/org#>
		prefix skos: <http://www.w3.org/2004/02/skos/core#>
		SELECT $this (COUNT(?mep) AS ?nbMep)
		WHERE {
		    ?mep a $this .
		    ?mep eponto:Status <http://data.europarl.europa.eu/authority/conceptStatus/ACTIVE> .
		}
		GROUP BY $this
		HAVING (?nbMep < 705)
		"""  
	]
.