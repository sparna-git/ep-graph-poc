@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix ep: <http://data.europarl.europa.eu/> .
@prefix eponto: <http://data.europarl.europa.eu/ontology/ep#> .
@prefix epsh: <http://data.europarl.europa.eu/shapes#> .
@prefix euvoc: <http://publications.europa.eu/ontology/euvoc#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix position: <http://data.europarl.europa.eu/authority/votePosition/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix qb: <http://purl.org/linked-data/cube#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix skosthes: <http://purl.org/iso25964/skos-thes#> .
@prefix skosxl: <http://www.w3.org/2008/05/skos-xl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://data.europarl.europa.eu/shapes/votes> a owl:Ontology;
  rdfs:label "Votes" .

epsh:Vote a sh:NodeShape;
  rdfs:comment "A Vote that took place within a sitting, identified with a vote number. Example URI \"/eli/dl/iPlMeeting/20191219/3\"";
  rdfs:label "Vote"@en, "Vote"@fr;
  sh:closed true;
  sh:ignoredProperties _:node1esit516mx3122 .

_:node1esit516mx3122 rdf:first rdf:type;
  rdf:rest rdf:nil .

epsh:Vote sh:nodeKind sh:IRI;
  sh:order 1;
  sh:pattern "^http://data.europarl.europa.eu/eli/dl/iPlMeeting/[0-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9]/[0-9][0-9]?$";
  sh:property epsh:P11, epsh:P12, epsh:P13, epsh:P14;
  sh:targetClass eponto:Vote .

epsh:MEPVote a sh:NodeShape;
  rdfs:comment "Represents the vote of a single MEP within a Vote. This is identified by the Vote number and the MEP ID. Example URI \"/eli/dl/iPlMeeting/20191219/3/MEP_342\"";
  rdfs:label "MEP Vote"@en, "Vote d’un MEP"@fr;
  sh:closed true;
  sh:ignoredProperties _:node1esit516mx3123 .

_:node1esit516mx3123 rdf:first rdf:type;
  rdf:rest rdf:nil .

epsh:MEPVote sh:nodeKind sh:IRI;
  sh:order 2;
  sh:pattern "^http://data.europarl.europa.eu/eli/dl/iPlMeeting/[0-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9]/[0-9][0-9]?/MEP_.*$";
  sh:property epsh:P18, epsh:P19, epsh:P20, epsh:P21;
  sh:targetClass eponto:MEPVote .

epsh:PlenarySitting a sh:NodeShape;
  rdfs:comment "A plenary sitting";
  sh:nodeKind sh:IRI;
  sh:pattern "^http://data.europarl.europa.eu/eli/dl/iPlMeeting/[0-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9]$" .

epsh:VotePosition a sh:NodeShape;
  rdfs:comment "A reference to a vote position (for, against, abstention)";
  sh:nodeKind sh:IRI .

epsh:PoliticalGroup a sh:NodeShape;
  rdfs:comment "A reference to a Political Group";
  sh:nodeKind sh:IRI;
  sh:pattern "^http://data.europarl.europa.eu/org/group/.*$" .

epsh:MEP a sh:NodeShape;
  rdfs:comment "A reference to an MEP";
  sh:nodeKind sh:IRI;
  sh:pattern "^http://data.europarl.europa.eu/person/MEP_.*$" .

epsh:P11 sh:datatype rdf:langString;
  sh:languageIn _:node1esit516mx3126 .

_:node1esit516mx3126 rdf:first "fr";
  rdf:rest rdf:nil .

epsh:P11 sh:maxCount 1;
  sh:minCount 1;
  sh:name "Name of the voted text (from RollCallVote.Description.Text)"@fr, "preferred label"@en;
  sh:nodeKind sh:Literal;
  sh:order 1;
  sh:path skos:prefLabel .

epsh:P12 sh:datatype xsd:integer;
  sh:maxCount 1;
  sh:minCount 1;
  sh:name "Number of the vote (from RollCallVote.Result/@Number)"@fr, "vote identifier"@en;
  sh:nodeKind sh:Literal;
  sh:order 2;
  sh:path eponto:voteIdentifier .

epsh:P13 sh:class eponto:MEPVote;
  sh:minCount 1;
  sh:name "Link to MEP votes in that Vote"@fr, "consists of MEP vote"@en;
  sh:nodeKind sh:IRI;
  sh:order 3;
  sh:path eponto:voteConsistsOfMEPVote .

epsh:P14 sh:maxCount 1;
  sh:minCount 1;
  sh:name "Link to plenary sitting during which the vote took place"@fr, "preferred labellenary sitting"@en;
  sh:node epsh:PlenarySitting;
  sh:nodeKind sh:IRI;
  sh:order 4;
  sh:path eponto:voteFormsPartOfPlenarySitting .

epsh:P18 sh:maxCount 1;
  sh:minCount 1;
  sh:name "Link to the MEP that voted"@fr, "mep"@en;
  sh:node epsh:MEP;
  sh:nodeKind sh:IRI;
  sh:order 1;
  sh:path eponto:voteByMEP .

epsh:P19 sh:in _:node1esit516mx3127 .

_:node1esit516mx3127 rdf:first position:for;
  rdf:rest _:node1esit516mx3128 .

_:node1esit516mx3128 rdf:first position:against;
  rdf:rest _:node1esit516mx3129 .

_:node1esit516mx3129 rdf:first position:abstention;
  rdf:rest rdf:nil .

epsh:P19 sh:maxCount 1;
  sh:minCount 1;
  sh:name "Indicates whether to vote is « for », « against » or « abstention »"@fr,
    "position"@en;
  sh:node epsh:VotePosition;
  sh:nodeKind sh:IRI;
  sh:order 2;
  sh:path eponto:votePosition .

epsh:P20 sh:maxCount 1;
  sh:minCount 1;
  sh:name "Link to political group to which the MEP belongs when the vote took place"@fr,
    "political group"@en;
  sh:node epsh:PoliticalGroup;
  sh:nodeKind sh:IRI;
  sh:order 3;
  sh:path eponto:voteFromPoliticalGroup .

epsh:P21 sh:class eponto:Vote;
  sh:maxCount 1;
  sh:minCount 1;
  sh:name "Link to the vote"@fr, "forms part of vote"@en;
  sh:nodeKind sh:IRI;
  sh:order 4;
  sh:path eponto:MEPVoteFormsPartOfVote .
