	owl:onDataRange xsd:string .

:Person :inputRule "CODICT input" ;
	:uriTemplate "ep-orgr:person/{ep-org:personId}" ;
	rdfs:definition "Person refers to a human being regarded as an individual."@en .
# 
# http://data.europarl.europa.eu/ontology/ep-org#PersonType

:PersonType a owl:Class ;
	owl:equivalentClass _:genid16 .

_:genid16 a owl:Restriction ;
