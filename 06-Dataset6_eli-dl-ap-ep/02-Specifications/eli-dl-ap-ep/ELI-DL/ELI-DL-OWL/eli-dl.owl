     -->

    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#activity_date -->

    <owl:DatatypeProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#activity_date">
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeActivity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:comment xml:lang="en">The date of an activity, to indicate a single date. Alternatively, if an activity spans a time range, &quot;start_date&quot; and &quot;end_date&quot; can be used to indicate the beginning and end of the activity.</rdfs:comment>
        <rdfs:label xml:lang="en">activity date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#activity_end_date -->

    <owl:DatatypeProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#activity_end_date">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeActivity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:comment xml:lang="en">The date at which a legislative activity ended.</rdfs:comment>
        <rdfs:label xml:lang="en">activity end date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#activity_label -->

    <owl:DatatypeProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#activity_label">
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeActivity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">A label on the activity, describing what is being carried out.</rdfs:comment>
