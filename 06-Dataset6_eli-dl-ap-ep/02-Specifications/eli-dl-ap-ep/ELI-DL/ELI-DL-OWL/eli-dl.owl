    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#foreseen_type_document -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#foreseen_type_document">
