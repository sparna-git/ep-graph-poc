        <rdfs:range rdf:resource="http://data.europa.eu/eli/ontology#ResourceType"/>
        <rdfs:comment xml:lang="en">Indicates the expected type of the legislation that will be the result of this legislative process.
The expected type of legislation can change over time.

The value must be taken in the ResourceType table, already used for the eli:type_document property.</rdfs:comment>
        <rdfs:label xml:lang="en">foreseen type of document</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#foresees_amendment_of -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#foresees_amendment_of">
