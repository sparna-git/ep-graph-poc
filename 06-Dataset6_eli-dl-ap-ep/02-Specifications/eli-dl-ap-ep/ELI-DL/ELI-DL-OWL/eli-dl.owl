    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#forms_part_of">
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P9i_forms_part_of"/>
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeActivity"/>
        <rdfs:range rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeActivity"/>
        <rdfs:comment xml:lang="en">Indicates another larger activity that encompasses this one. 

Example : the activity &quot;Readng in commission of legislative process 384&quot; forms part of &quot;First lecture in assembl√©e nationale of legislative process 384&quot;, which itself forms part &quot;Legislative process number 384&quot;.</rdfs:comment>
        <rdfs:label xml:lang="en">forms part of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#had_legal_basis -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#had_legal_basis">
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P33_used_specific_technique"/>
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeActivity"/>
        <rdfs:range rdf:resource="http://data.europa.eu/eli/ontology#LegalResource"/>
        <rdfs:comment xml:lang="en">Indicates an act that specified the legislative workflow in which this legal activity takes place.

Example : &quot;The Signature by the President of the European Parliament and the President of the Council of Procedure 2016/0382/COD had the legal basis TFEU/art 294  and TFEU/art 194 par 2&quot;.</rdfs:comment>
        <rdfs:label xml:lang="en">had legal basis</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#had_participant_organization -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#had_participant_organization">
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P11_had_participant"/>
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeActivity"/>
        <rdfs:range rdf:resource="http://data.europa.eu/eli/ontology#Organization"/>
        <rdfs:comment xml:lang="en">An organization participating in an activity, but not necessarily responsible for it.</rdfs:comment>
        <rdfs:label xml:lang="en">had participant organization</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#had_participant_person -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#had_participant_person">
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P11_had_participant"/>
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeActivity"/>
        <rdfs:range rdf:resource="http://data.europa.eu/eli/ontology#Person"/>
        <rdfs:comment xml:lang="en">A person participating in a legal activity. A more precise property exists to indicate the responsible person of an activity.

Example : members in a commision work, speaker during a plenary session</rdfs:comment>
        <rdfs:label xml:lang="en">had participant person</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#had_responsible_organization -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#had_responsible_organization">
        <rdfs:subPropertyOf rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#had_participant_organization"/>
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P14_carried_out_by"/>
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeActivity"/>
        <rdfs:range rdf:resource="http://data.europa.eu/eli/ontology#Organization"/>
        <rdfs:comment xml:lang="en">The organization responsible for carrying out the activity. This can be the institution in which the activity happened, or a service, a commission.

Example :
The activity &quot;1st reading of 2016/0382/COD&quot; had responsible organization European Parliament.
The EU procedure 2016/0382/COD had responsible organization Directorate-General for Energy.</rdfs:comment>
        <rdfs:label xml:lang="en">had responsible organization</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#had_responsible_person -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#had_responsible_person">
        <rdfs:subPropertyOf rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#had_participant_person"/>
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P14_carried_out_by"/>
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeActivity"/>
        <rdfs:range rdf:resource="http://data.europa.eu/eli/ontology#Person"/>
        <rdfs:comment xml:lang="en">The responsible person of an activity.

Example : the &quot;rapporteur&quot; of a text in a commision.</rdfs:comment>
        <rdfs:label xml:lang="en">had responsible person</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#includes_amendment -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#includes_amendment">
        <rdfs:subPropertyOf rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#refers_to"/>
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#DraftLegislationWork"/>
        <rdfs:range rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#AmendmentToDraftLegislationWork"/>
        <rdfs:comment xml:lang="en">Indicate an amendment that was passed and included in this version of the legislation draft. This property is meant to refer to individual amendments that have been voted to be integrated in the text of the law.</rdfs:comment>
        <rdfs:label xml:lang="en">includes amendment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#involved_work -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#involved_work">
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P16_used_specific_object"/>
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeActivity"/>
        <rdfs:range rdf:resource="http://data.europa.eu/eli/ontology#Work"/>
        <rdfs:comment xml:lang="en">Indicates that a LegislativeActivity somehow used a work (document) to be conducted, without indicating how it was used.
this covers both the documents used as &quot;input&quot; of the activity and also the document produced during the activity as &quot;output&quot;.
More precise properties exist to indicate if the works should be considered &quot;inputs&quot; or &quot;outputs&quot; of the activity.</rdfs:comment>
        <rdfs:label xml:lang="en">involved work</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#is_referred_to_by -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#is_referred_to_by">
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P67i_is_referred_to_by"/>
        <owl:inverseOf rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#refers_to"/>
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/ontology#Work"/>
        <rdfs:range rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeProcessWork"/>
        <rdfs:comment xml:lang="en">Indicates a legislative process work that refers to this work.</rdfs:comment>
        <rdfs:label xml:lang="en">is referred to by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#is_subject_of -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#is_subject_of">
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P129i_is_subject_of"/>
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeProcess"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment xml:lang="en">A webpage or resource that describe a legislative process. This property is meant to crosslink the websites of different institutions that describe the same legislative process.

Example : the page of the legislation process in the OJ website, the page of the process in the Senate website, the page of the process in the State Council website.</rdfs:comment>
        <rdfs:label xml:lang="en">is subject of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#latest_activity -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#latest_activity">
        <rdfs:subPropertyOf rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#consists_of"/>
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeProcess"/>
        <rdfs:range rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeActivity"/>
        <rdfs:comment xml:lang="en">Indicates the latest activity that took place within a legislative process. This is used to indicate to a reader the latest stage of a process.
Depending on the point of view, this can be used to point to the ongoing activity (not finished yet), or to the latest activity that actually completed.</rdfs:comment>
        <rdfs:label xml:lang="en">latest activity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#legislative_process_keyword -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#legislative_process_keyword">
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P129_is_about"/>
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeProcess"/>
        <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:comment xml:lang="en">The keywords, subject or categories for a legislative process.

Example : Procedure 2016/0382/COD has Eurovoc keywords energy consumption, atmospheric pollution, renewable resources, EU programme, environmental cooperation, greenhouse gas, reduction of gas emissions, energy saving and renewable energy.</rdfs:comment>
        <rdfs:label xml:lang="en">legislative process keyword</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#legislative_process_status -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#legislative_process_status">
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P2_has_type"/>
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeProcess"/>
        <rdfs:range rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeProcessStatus"/>
        <rdfs:comment xml:lang="en">The status of a legislative process, in one of the possible values included in this model.</rdfs:comment>
        <rdfs:label xml:lang="en">legislative process status</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#legislative_process_type -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#legislative_process_type">
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P2_has_type"/>
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeProcess"/>
        <rdfs:range rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeProcessType"/>
        <rdfs:comment xml:lang="en">The type of a legislative process. The type of a legislative process often determine the workflow being followed to create the law.

The list of possible values need to be defined by each Member State.

Example : &quot;Projet de loi&quot;, &quot;Proposition de loi&quot;, &quot;Revision of the Constitution&quot;, &quot;Public Bill&quot;.</rdfs:comment>
        <rdfs:label xml:lang="en">legislative process type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#legislative_process_work_creator -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#legislative_process_work_creator">
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeProcessWork"/>
        <rdfs:range rdf:resource="http://data.europa.eu/eli/ontology#Agent"/>
        <rdfs:comment xml:lang="en">Indicates the creator of legislative process work.</rdfs:comment>
        <rdfs:label xml:lang="en">legislative process work creator</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#legislative_process_work_type -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#legislative_process_work_type">
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P2_has_type"/>
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeProcessWork"/>
        <rdfs:range rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeProcessWorkType"/>
        <rdfs:comment xml:lang="en">The type of a legislative process work. The list of possible types need to be defined by each Member States.

Example : &quot;Opinition from the economic commitee&quot;, &quot;Impact analysis&quot;, &quot;text of the bill&quot;, &quot;debate recording&quot;, &quot;amendment&quot;.</rdfs:comment>
        <rdfs:label xml:lang="en">legislative process work type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#legislative_process_work_version -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#legislative_process_work_version">
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P2_has_type"/>
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeProcessWork"/>
        <rdfs:range rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeProcessStatus"/>
        <rdfs:comment xml:lang="en">The version, or status, in which the legislative process work is currently : preparatory, final, etc.</rdfs:comment>
        <rdfs:label xml:lang="en">legislative process work version</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#motivated -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#motivated">
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P17i_motivated"/>
        <owl:inverseOf rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#was_motivated_by"/>
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeActivity"/>
        <rdfs:range rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeActivity"/>
        <rdfs:comment xml:lang="en">Annother activity that was &quot;triggered&quot; or &quot;logically followed&quot; this one.

Example : the request for an opinion motivated the writing of an opinion.</rdfs:comment>
        <rdfs:label xml:lang="en">motivated</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#occured_at_stage -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#occured_at_stage">
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P2_has_type"/>
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeActivity"/>
        <rdfs:range rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeProcessStage"/>
        <rdfs:comment xml:lang="en">The legislative stage at which an activity occured.

The description of the stages themselves is left open for implementers.

Example : The activity &quot;Presentation of Bill 78 of 2018&quot; corresponds to stage &quot;Presentation to the House&quot;. The activity &quot;1√®re lecture - Examen en commission du projet de loi 323&quot; corresponds to stage &quot;Examen en commission&quot;.</rdfs:comment>
        <rdfs:label xml:lang="en">occured at stage</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#produced_manifestation -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#produced_manifestation">
        <rdfs:subPropertyOf rdf:resource="http://iflastandards.info/ns/fr/frbr/frbroo/R18_created"/>
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeActivity"/>
        <rdfs:range rdf:resource="http://data.europa.eu/eli/ontology#ManifestationProductType"/>
        <rdfs:comment xml:lang="en">Indicates that a legal activity created a Manifestation.
This property is mostly here for compatibility with FRBRoo, but implementations would prefer using created_a_realization_of to link to the Work level.
This could indicate that some Manifestation/Formats of the final LegalResource were created by the legislative process, and some others were generated afterwards.</rdfs:comment>
        <rdfs:label xml:lang="en">produced manifestation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#recorded_in_realization_of -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#recorded_in_realization_of">
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeActivity"/>
        <rdfs:range rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeProcessWork"/>
        <rdfs:comment xml:lang="en">Indicates that an activity was recorded in a related work.

Note : this property is shortcut for an implicit FRBR &quot;Recording Event&quot; that would have created the Recording of the event.</rdfs:comment>
        <rdfs:label xml:lang="en">recorded in realization of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#refers_to -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#refers_to">
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P67_refers_to"/>
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeProcessWork"/>
        <rdfs:range rdf:resource="http://data.europa.eu/eli/ontology#Work"/>
        <rdfs:comment xml:lang="en">Any work that a legislative process work can refer to.

Note that specific subproperties exist to describe future legal impacts (&quot;foresees_xxxx&quot; links) and links between amendments and draft legislation.

Example : an opinion refers to the specific version of the draft legislation on which it is based.</rdfs:comment>
        <rdfs:label xml:lang="en">refers to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#was_motivated_by -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#was_motivated_by">
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P17_was_motivated_by"/>
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeActivity"/>
        <rdfs:range rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeActivity"/>
        <rdfs:comment xml:lang="en">Another activity that &quot;triggered&quot; or &quot;logically preceded&quot; this activity.

Example : the writing of an opinion was motivated by the transmission of a request for an opinion by the legislation service.</rdfs:comment>
        <rdfs:label xml:lang="en">was motivated by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#was_submitted_by -->

    <owl:ObjectProperty rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#was_submitted_by">
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P11_had_participant"/>
        <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P17_was_motivated_by"/>
        <rdfs:domain rdf:resource="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeProcess"/>
        <rdfs:range rdf:resource="http://data.europa.eu/eli/ontology#Agent"/>
        <rdfs:comment xml:lang="en">Indicates the organization or person that submitted a legislative process to the parliament (this could be the government, the President, the Senate, a group of members of the parliament, a parliamentary committee, a group of citizens, etc.).</rdfs:comment>
        <rdfs:label xml:lang="en">was submitted by</rdfs:label>
    </owl:ObjectProperty>
    


