    <!-- http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeProcessStatus-Successful -->

    <owl:NamedIndividual rdf:about="http://data.europa.eu/eli/eli-draft-legislation-ontology#LegislativeProcessStatus-Successful">
